---
import "../styles/global.css";
import Metas from "./seo/Metas.astro";
import Favicons from "./seo/Favicons.astro";

const {
  title = "Productized Service Agencies",
  description = "Find the best productized service agencies for your business needs.",
  url = new URL(Astro.url.pathname, Astro.site),
  socialImage = "/social.jpg",
  twitterImage = "/twitter.jpg",
  author = "Productized Studio"
} = Astro.props;
---

<Metas
  title={title}
  description={description}
  url={url}
  socialImage={socialImage}
  twitterImage={twitterImage}
  author={author}
/>
<Favicons/>
<!-- HTML in your document's head -->
<link rel="preconnect" href="https://rsms.me/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
<link
  href="https://api.fontshare.com/v2/css?f[]=jet-brains-mono@1,2&display=swap"
  rel="stylesheet"
/>
<!-- Alpine.js Plugins -->
<script defer src="https://unpkg.com/@alpinejs/focus@3.10.3/dist/cdn.min.js"
></script>
<script defer src="https://cdn.jsdelivr.net/npm/@alpinejs/intersect@3.x.x/dist/cdn.min.js"></script>
<!-- Alpine.js Core -->
<script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>
<!---- Smoothscroll -->
<script
  defer
  src="https://unpkg.com/smoothscroll-polyfill@0.4.4/dist/smoothscroll.js"
></script>
<!---- Keen slider -->
<!--
  Keen slider CSS if needed
  <link
  href="https://cdn.jsdelivr.net/npm/keen-slider@6.8.6/keen-slider.min.css"
  rel="stylesheet"
/>
<!-- Rest of your head content -->
